<form method="post" action="/join" onsubmit="return Validation()">
  <table id="signUp">
    <tr>
      <td colspan="2">
        <h1>회원가입</h1>
      </td>
    </tr>
    <tr>
      <th><label for="name">이름</label></th>
      <td><input type="text" name="name" id="name"></td>
    </tr>
    <tr>
      <th><label for="address">주소</label></th>
      <td><input type="text" name="address" id="address"></td>
    </tr>
    <tr>
      <th><label for="birth">생년월일</label></th>
      <td><input type="text" name="birth" id="num"></td>
    </tr>
    <tr>
      <th><label for="id">아이디</label></th>
      <td><input type="text" name="id" id="id"></td>
    </tr>
    <tr>
      <th><label for="pw">비밀번호</label></th>
      <td><input type="password" name="pw" id="pwd"></td>
    </tr>
    <tr>
      <th><label for="repw">비밀번확인</label></th>
      <td><input type="password" name="repw" id="pwd2"></td>
    </tr>
    <tr>
      <th><label for="repw">Email</label></th>
      <td><input type="text" name="mail" id="mail"></td>
    </tr>
    <tr>
      <td colspan="2"><input type="submit" value="회원가입" class="join_sub"></td>
    </tr>
  </table>
</form>
<script src="js/join.js"></script>